BEGINSUB name=SpecialHelper
	IF not($_CurrentDatabase=$Editbox)
		DATABASE name=$Editbox StopStartupMacro=Y
	ENDIF

	WHILE TRUE
		$FormExit = Form($SpecialHelper,"")
		BEGINCASE
		CASE $FormExit = "TenYearBadge"
			$Help_type = "Ten Year Event"
			$_sql = "cachetype='E' AND (PlacedDate in ('2010-04-30','2010-05-01','2010-05-02','2011-05-03')) AND UserNote not like '%BadgeGen10YearEvent%' AND UserNote not like '%BadgeGenNot10YearEvent%' and Found"
			$Help_tag = "BadgeGen10YearEvent"
			$On_no_results = "BadgeGen searched for all event caches on dates between April 30 and May 3, 2010 that weren't already tagged with 'BadgeGen10YearEvent' in the user note and came up empty."
			GOSUB name=HELP
		CASE $FormExit = "NightCacheBadge"
			$Help_type = "Night Cache"
			$_sql = "(((code in (select acode from attributes where aid = 52 and ainc)) ))   AND UserNote not like '%BadgeGenNightCache%' AND UserNote not like '%BadgeGenNotNightCache%' and found"
			$Help_tag = "BadgeGenNightCache"
			$On_no_results = "BadgeGen searched for all caches with the 'Night Cache' attribute that weren't already tagged with 'BadgeGenNightCache' in the user note and came up empty."
			GOSUB name=HELP
		CASE $FormExit = "ScubaBadge"
			$Help_type = "Scuba Cache"
			$_sql = "(((code in (select acode from attributes where aid = 5 and ainc)) ))   AND UserNote not like '%BadgeGenScubaCache%' AND UserNote not like '%BadgeGenNotScubaCache%' and found"
			$Help_tag = "BadgeGenScubaCache"
			$On_no_results = "BadgeGen searched for all caches with the 'Scuba Gear Required' attribute that weren't already tagged with 'BadgeGenScubaCache' in the user note and came up empty."
			GOSUB name=HELP
		CASE $FormExit = "SystemExit"
			BREAK

		ENDCASE
	ENDWHILE



	BEGINSUB name=HELP

		$Help_NotTag = Replace("BadgeGen","BadgeGenNot",$Help_tag)
		MFILTER Where=$_sql
		IF $_FilterCount>0
			GOTO position=top
			$num=0
			$total = $_FilterCount
			$count = 1
			WHILE Not($_EOL) AND $num=0
				$CacheInfo = EditForm($CacheInfo,"CacheName","Caption","$d_Code: $d_name")
				$CacheInfo = EditForm($CacheInfo,"Groupbox1","Caption","Is this cache a(n) $Help_type")
				$UserLog = Sqlite("sql","select lText from LogMemo where lLogID=(Select lLogID from logs where lParent='$d_code' and lIsOwner and g_foundlog(lType))")
				$No=TRUE
				$Yes=FALSE
				WHILE TRUE
					$CacheInfoExit = Form($cacheInfo,"")
					BEGINCASE
					CASE $CacheInfoExit = "Button1"
						IF $Yes
							$d_UserNote = $d_UserNote + $_NewLine + $_NewLine + "<br><br> $Help_tag"
						ELSE
							$d_UserNote = $d_UserNote + $_NewLine + $_NewLine + "<br><br> $Help_Nottag"
						ENDIF
						BREAK
					CASE $CacheInfoExit = "SystemExit"
						$num=1
						BREAK
					CASE $CacheInfoExit = "Button2"
						WEB url="www.coord.info/$d_code"
					ENDCASE
				ENDWHILE
				GOTO position=next
				$No=TRUE
				$Yes=FALSE
				SHOWSTATUS msg="Possible cache $current of $total"
				$count = $count + 1
			ENDWHILE
		ELSE
			MSGOK msg=$On_no_results
		ENDIF

	ENDSUB

ENDSUB

