	#Start listing conditions

	###
	$condition = "1 point per 100 geocaches found."
	$data_pass = "found"
	$comp = $geoc
	$num_temp = ($comp/100) - Frac($comp/100)
	IF $comp >= 100
	 $completion_status = "p"
	ENDIF
	GOSUB name=update

	###
	$condition = "2 points per event hosted (max. 30pt)"
	$data_pass = "hosted"
	$comp = $host
	$num_temp = $comp * 2
		IF $comp >= 1
		$completion_status = "p"
	ENDIF
	IF $num_temp >= 30
		$num_temp = 30
		$completion_status = "d"
	ENDIF
	GOSUB name=update

