	$condition = "20 points for finding caches in 15 states <u>OR</u> 5 countries."
	IF $State >= 15
		$num_temp = 20
		$comp = $state
		$data_pass = "states"
		$completion_status = "d"
	ENDIF
	IF $coun >= 5
		$num_temp = 20
		$comp = $coun
		$data_pass = "countries"
		$completion_status = "d"
	ENDIF
